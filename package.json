{
  "name": "DMSAdminApp",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap/cds": "^7",
    "@sap/cds-hana": "^2",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.21",
    "@sap/hana-client": "2.18.27",
    "express": "^4",
    "form-data": "4.0.0"
  },
  "devDependencies": {
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^7",
    "@sap/ux-specification": "^1.108.18"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-dmsuiapp": "cds watch --open dmsuiapp/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "db": "hana",
      "DMS_Token": {
        "kind": "rest",
        "credentials": {
          "destination": "DMS_Token",
          "path": "/oauth/token"
        }
      },
      "DMS_Dest": {
        "kind": "rest",
        "credentials": {
          "destination": "DMS_Dest",
          "path": "/"
        }
      }
    },
    "sapux": [
      "app/dmsuiapp"
    ]
  }